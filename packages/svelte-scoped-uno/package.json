    "unocss": "^0.50.6"
  },
  "devDependencies": {
    "@iconify-json/logos": "^1.1.23",
    "@vitest/ui": "^0.29.8",
    "prettier": "^2.8.7",
    "prettier-plugin-svelte": "^2.10.0",
    "svelte": "^3.57.0",
    "typescript": "^5.0.2",
    "unbuild": "^1.0.2",
    "vite": "^4.2.1",
    "vitest": "^0.29.8"
  }

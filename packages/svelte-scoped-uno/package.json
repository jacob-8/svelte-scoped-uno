  },
  "devDependencies": {
    "@iconify-json/logos": "^1.1.23",
    "@unocss/preset-icons": "^0.50.6",
    "@unocss/preset-uno": "^0.50.6",
    "@vitest/ui": "^0.29.3",
    "prettier": "^2.8.4",
    "prettier-plugin-svelte": "^2.9.0",
    "svelte": "^3.57.0",
    "typescript": "^4.9.4",
    "unbuild": "^1.0.2",
    "vite": "^4.2.0",
    "vitest": "^0.29.3"
  }
}